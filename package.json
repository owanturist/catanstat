{
  "private": true,
  "name": "catanstat",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean:build": "rm -rf ./build",
    "clean:cache": "rm -rf ./.parcel-cache",
    "copy:index-200": "cp build/index.html build/200.html",
    "dev": "parcel public/index.html --port 3000 --dist-dir .tmp-dev",
    "postdev": "rm -rf ./.tmp-dev",
    "prebuild": "npm run clean:cache && npm run clean:build",
    "build": "parcel build public/index.html --dist-dir build",
    "postbuild": "npm run copy:index-200",
    "analyze": "npm run build -- --reporter @parcel/reporter-bundle-analyzer",
    "ts": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owanturist/catanstat.git"
