{
  "private": true,
  "name": "catanstat",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean:build": "rm -rf ./build",
    "clean:cache": "rm -rf ./.parcel-cache",
    "copy:index-200": "cp build/index.html build/200.html",
    "predev": "npm run clean:cache",
    "dev": "parcel public/index.html --port 3000 --dist-dir .tmp-dev",
    "postdev": "rm -rf ./.tmp-dev",
    "prebuild": "npm run clean:cache && npm run clean:build",
    "build": "parcel build public/index.html --dist-dir build",
    "postbuild": "npm run copy:index-200",
    "preui": "npm run clean:cache",
    "ui": "parcel public/stories.html --port 5000 --dist-dir .tmp-stories",
    "postui": "rm -rf ./.tmp-stories",
    "preui:build": "npm run clean:cache && npm run clean:build",
    "ui:build": "parcel build public/stories.html --dist-dir build",
    "postui:build": "mv build/stories.html build/index.html && npm run copy:index-200",
    "review": "elm-review",
    "review:fix": "elm-review --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owanturist/catanstat.git"
  },
  "keywords": [
    "elm"
  ],
  "author": "Anton Ovechkin <owanturist@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/owanturist/catanstat/issues"
  },
  "homepage": "https://github.com/owanturist/catanstat#readme",
  "devDependencies": {
    "@parcel/transformer-elm": "^2.0.0",
    "autoprefixer": "^10.3.7",
    "elm": "^0.19.1-3",
    "elm-analyse": "^0.16.5",
    "elm-format": "^0.8.2",
    "elm-review": "^2.6.1",
    "parcel": "^2.0.0",
    "postcss": "^8.3.9",
    "postcss-import": "^14.0.2",
    "prettier": "^2.4.1"
  },
  "dependencies": {
    "tailwindcss": "^2.2.17"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead"
}
